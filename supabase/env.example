# Supabase Configuration
# Copy this file to .env.local and fill in your Supabase project details

# Supabase Project URL
VITE_SUPABASE_URL=your_supabase_project_url_here

# Supabase Anonymous Key
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: Supabase Service Role Key (for admin operations)
VITE_SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Application Configuration
VITE_APP_NAME=Social Responsibility Council
VITE_APP_URL=http://localhost:5173

# Email Configuration (if using custom email service)
VITE_EMAIL_SERVICE_URL=your_email_service_url
VITE_EMAIL_API_KEY=your_email_api_key

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here

# Development
NODE_ENV=development
